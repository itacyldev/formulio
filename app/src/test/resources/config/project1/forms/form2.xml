<main name="Agents form" description="An example of short description" id="form2" repo="agents">
    <edit>
        <form properties="all">
            <autocomplete forceSelection="true" id="provincia" label="provincia" repo="provRepo">
                <options labelExpression="${entity.name}" labelFilteringProperty="name"
                    valueProperty="id" />
            </autocomplete>
            <autocomplete forceSelection="true" id="provmuni" label="municipio" repo="muniRepo"
                value="${entity.provmuni}">
                <options labelExpression="${entity.name}" labelFilteringProperty="name"
                    valueProperty="provmuni" />
            </autocomplete>
        </form>
    </edit>
</main>
