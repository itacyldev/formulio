<main name="Superheroes" description="Simple form with image example." id="form3" repo="superHRepo">
    <edit>
        <form>
            <input value="${entity.name}" label="Name"/>
            <image value="${entity.image}" repo="superHImages" />
            <image value="${entity.image_thumb}" repo="superHImages" converter="b64Image" />
        </form>
    </edit>
    <list>
        <datatable properties="name, real_name, age"/>
    </list>
</main>
