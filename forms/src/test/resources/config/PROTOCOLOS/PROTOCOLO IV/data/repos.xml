<repos>
    <repo dbFile="calidad.sqlite" id="provRepo" dbTable="provincia" />
    <repo dbFile="calidad.sqlite" id="muniRepo" dbTable="municipio" />
    <repo dbFile="calidad.sqlite" id="grupoProductoRepo" dbTable="grupo_producto" />
    <repo dbFile="calidad.sqlite" id="auditoriaRepo" dbTable="auditoria">
        <mapping fk="prov_id" property="provincia" repo="provRepo" updatable="false"/>
        <mapping fk="provmuni_id" property="municipio" repo="muniRepo" updatable="false"/>
        <mapping fk="grupo_producto_id" property="grupoProducto" repo="grupoProductoRepo" updatable="false"/>
    </repo>
    <repo dbFile="calidad.sqlite" id="preguntaRepo" dbTable="pregunta" />
    <repo dbFile="calidad.sqlite" id="preguntaAuditoriaRepo" dbTable="pregunta_auditoria">
        <mapping fk="auditoria_id" property="auditoria" repo="auditoriaRepo"/>
        <mapping fk="pregunta_id" property="pregunta" repo="preguntaRepo"/>
    </repo>
</repos>

