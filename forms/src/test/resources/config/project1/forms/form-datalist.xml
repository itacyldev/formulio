<main name="Datalist form" description="An example of short description" id="form6"
    repo="superHRepo">
    <list>
        <form>
            <datalist>
                <datalistitem>
                    <card expandable="true" expanded="false" label="${entity.name}">
                        <head id="title" label="label_title" value="${entity.name}" />
                        <head id="subtitle" label="label_subtitle" value="${entity.real_name}" />"
                        <image inputType="0" repo="superHImages" value="${entity.image}" />
                    </card>
                </datalistitem>
            </datalist>
        </form>
    </list>
</main>
