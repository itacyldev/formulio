<main name="Form Datalist" description="An example of short description" id="formDatalist" repo="superHRepo">
    <list name="Form Datalist">
        <form>
            <datalist>
                <datalistitem>
                    <card expandable="true" expanded="false" label="${entity.name}">
                        <p name="title" value="${entity.name}" />
                        <p name="subtitle" value="${entity.real_name}" />"
                        <image inputType="0" repo="superHImages" value="${entity.image}" />
                    </card>
                </datalistitem>
            </datalist>
        </form>
    </list>
</main>
