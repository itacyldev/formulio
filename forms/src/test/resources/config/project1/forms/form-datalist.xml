<main name="Datalist form" description="An example of short description" id="form3"
    repo="superHRepo">
    <list>
        <form>
            <datalist template="card_template_1">
                <card template="card_template_2" title="${entity.name}">
                    <head id="title" label="label_title" value="${entity.name}" />
                    <head id="subtitle" label="label_subtitle" value="${entity.real_name}" />"
                    <image inputType="0" repo="superHImages" value="${entity.image}" />
                </card>
            </datalist>
        </form>
    </list>
</main>
